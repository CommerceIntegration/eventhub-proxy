{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "username": {
            "value" : "[ADMIN-USERNAME]"
        },
        "password": {
            "value" : "[ADMIN-PASSWORD]"
        },
        "vm_image_uri": {
            "value": "/subscriptions/[SUBSCRIPTION-GUID]/resourceGroups/[RESOURCE-GROUP-NAME]/providers/Microsoft.Compute/images/[IMAGE-NAME]"
        },
        "public_ip_uri": {
            "value": "/subscriptions/[SUBSCRIPTION-GUID]/resourceGroups/[RESOURCE-GROUP-NAME]/providers/Microsoft.Network/publicIPAddresses/[IP-NAME]"
        }
    }
}